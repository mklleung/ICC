doctype html
html(lang="en")
  head
    title= title
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no")
    link(rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" integrity="sha384-WskhaSGFgHYWDcbwN70/dfYBj47jz9qbsMId/iRN3ewGhXQFZCSftd1LZCfmhktB" crossorigin="anonymous")
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css')
    meta(name='viewport' content='width=device-width, initial-scale=1')
    block head
  body
    ul.nav.navbar-nav.navbar-right
    #mySidenav.sidenav
      a.closebtn(href='javascript:void(0)' onclick='closeNav()') &times;
      h3.underline.white.mono Brandeis
      h3.underline.white.mono Intercultural
      h3.underline.white.mono Center
      a.mono(href="/") Home
      a.mono(href='/clubs') Clubs
      a.mono(href='/eBoards') E-Boards
      a.mono(href='/allPosts') All Posts
      a.mono(href='/aboutICC') About ICC

      if loggedIn
        a.mono(href='/addPosts') Add a Post
        if status=='admin'
          a.mono(href='/admin') Admin
        if status=='jsa'
          a.mono(href='/jsaManage') Manage Club Page
          a.mono(href='/clubs/jsa') JSA Page
        a.mono(href='/logout') log Out
      else
        a.mono(href='/logins') Club Log In
        a.mono(href='/allPages') All Pages
      br
      br
      br
      br
      br
      br
      br
      br
      br
      br
      br
      br
      br
      br
      p.white.footer.mono Michael Leung 2018 (Voice, Web, and Mobile Apps Justice Brandeis Semester)
    .icon-bar
      a.facebook(href="https://www.facebook.com/BrandeisICC/")
        i.fa.fa-facebook
      a.twitter(href="https://twitter.com/brandeisicc")
        i.fa.fa-twitter
      a.google(href="http://www.brandeis.edu/studentlife/icc/")
        i.fa.fa-google
      a.linkedin(href="https://www.linkedin.com/school/brandeis-university/")
        i.fa.fa-linkedin
      a.youtube(href="https://www.youtube.com/user/BrandeisUniversity")
        i.fa.fa-youtube
    span(style='font-size:30px;cursor:pointer' onclick='openNav()') &#x2630;

